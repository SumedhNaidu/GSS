<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<runtime>
		<!-- Binding Redirects to ensure compatibility with required versions of System.Runtime -->
		<assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
			<dependentAssembly>
				<assemblyIdentity name="System.Runtime" publicKeyToken="b03f5f7f11d50a3a" culture="neutral"/>
				<!-- Redirect to a compatible version of System.Runtime for .NET Framework -->
				<bindingRedirect oldVersion="0.0.0.0-4.3.0.0" newVersion="4.3.0"/>
			</dependentAssembly>
		</assemblyBinding>
	</runtime>

	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
</configuration>
